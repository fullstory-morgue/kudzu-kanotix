#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "keyrði loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "nefndi %s sem %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "framkvæma einungis 'örugga' vélbúnaðarleit sem truflar ekki vélbúnað"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "setja tímatakmörk í sekúndum"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "leita einungis, prenta það sem finnst á staðalúttak"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "leita einungis á tiltekinni braut"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "leita einungis að tilteknum hóp"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "lesa niðurstöður vélbúnaðarleitar úr skrá"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "skrá sem inniheldur vélbúnaðarupplýsingar"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "leita að reklum fyrir tiltekna kjarnaútgáfu"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "útgáfa kjarna"

#: ../hwconf.c:925
#, fuzzy
msgid "read probed hardware from a socket"
msgstr "lesa niðurstöður vélbúnaðarleitar úr skrá"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr ""

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"VILLA - Þú verður að vera rótarnotandi til að keyra kudzu.\n"

