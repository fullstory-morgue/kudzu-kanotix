#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "eseguito loadkey %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "alias %s come %s"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "Eseguire solo rilevamenti che non compromettono l'hardware"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "impostare il timeout in secondi"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "solo rilevamento, visualizzare le informazioni sullo standard output"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "rilevare solo il 'bus' specificato"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "rilevare solo la 'classe' specificata"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "leggere l'hardware rilevato da un file"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "file in cui trovare le informazioni sull'hardware"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "cerca i moduli per una particolare versione della kernel"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "versione della kernel"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "leggere l'hardware rilevato da un socket"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr "fa una configurazione che non richieda input utente"

#: ../hwconf.c:951
