#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "uruchomiono loadkeys %s"

#: ../hwconf.c:532 ../hwconf.c:548 ../hwconf.c:581
#, c-format
msgid "aliased %s as %s"
msgstr "utworzono alias %s do %s"

#: ../hwconf.c:828
msgid "do only 'safe' probes that won't disturb hardware"
msgstr ""
"wykonaj jedynie \"bezpieczne\" testy, które nie spowodują zaburzeń w pracy "
"sprzętu"

#: ../hwconf.c:832
msgid "set timeout in seconds"
msgstr "ustaw czas oczekiwania w sekundach"

#: ../hwconf.c:835
msgid "probe only, print information to stdout"
msgstr "tylko szukaj, wyświetl informację na standardowe wyjście"

#: ../hwconf.c:839
msgid "probe only the specified 'bus'"
msgstr "szukaj tylko określonej \"magistrali\""

#: ../hwconf.c:843
msgid "probe only for the specified 'class'"
msgstr "szukaj tylko określonej \"klasie\""

#: ../hwconf.c:847
msgid "read probed hardware from a file"
msgstr "odczytaj wyszukiwany sprzęt z pliku"

#: ../hwconf.c:848
msgid "file to read hardware info from"
msgstr "plik z informacją o sprzęcie do odczytania"

#: ../hwconf.c:851
msgid "search for modules for a particular kernel version"
msgstr "szukaj modułów dla podanej wersji jądra"

#: ../hwconf.c:852
msgid "kernel version"
msgstr "wersja jądra"

#: ../hwconf.c:855
msgid "read probed hardware from a socket"
msgstr "odczytaj wyszukiwany sprzęt z gniazda"

#: ../hwconf.c:859
msgid "do configuration that doesn't require user input"
msgstr "utwórz konfigurację, która nie wymaga udziału użytkownika"

#: ../hwconf.c:881
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"BŁĄD - musisz być rootem, aby uruchomić kudzu.\n"
