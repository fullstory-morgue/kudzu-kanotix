#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "pokrenut loadkeys %s"

#: ../hwconf.c:532 ../hwconf.c:548 ../hwconf.c:581
#, c-format
msgid "aliased %s as %s"
msgstr "%s predstavljen kao %s"

#: ../hwconf.c:828
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "izvršavaj samo „sigurne“ testove koji neće uznemiravati hardver"

#: ../hwconf.c:832
msgid "set timeout in seconds"
msgstr "postavi vreme isteka u sekundama"

#: ../hwconf.c:835
msgid "probe only, print information to stdout"
msgstr "samo testiraj, štampaj informacije na stdout"

#: ../hwconf.c:839
msgid "probe only the specified 'bus'"
msgstr "testiraj samo navedenu „magistralu“"

#: ../hwconf.c:843
msgid "probe only for the specified 'class'"
msgstr "testiraj samo navedenu „klasu“"

#: ../hwconf.c:847
msgid "read probed hardware from a file"
msgstr "učitaj testiran hardver iz datoteke"

#: ../hwconf.c:848
msgid "file to read hardware info from"
msgstr "datoteka iz koje se učitavaju informacije o hardveru"

#: ../hwconf.c:851
msgid "search for modules for a particular kernel version"
msgstr "potraži module za određenu verziju jezgra"

#: ../hwconf.c:852
msgid "kernel version"
msgstr "verzija jezgra"

#: ../hwconf.c:855
msgid "read probed hardware from a socket"
msgstr "učitaj testiran hardver sa priključka"

#: ../hwconf.c:859
msgid "do configuration that doesn't require user input"
msgstr "izvršavaj podešavanja koja ne zahtevaju korisnički unos"

#: ../hwconf.c:881
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"GREŠKA - Morate biti root da bi pokrenuli kudzu.\n"
