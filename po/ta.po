#: ../hwconf.c:126
#, c-format
msgid "ran loadkeys %s"
msgstr "ran loadkeys %s"

#: ../hwconf.c:533 ../hwconf.c:549 ../hwconf.c:579 ../hwconf.c:611
#, c-format
msgid "aliased %s as %s"
msgstr "%s யை %s ஆக மாற்றியுள்ளது"

#: ../hwconf.c:898
msgid "do only 'safe' probes that won't disturb hardware"
msgstr "ஏற்கெனவே உள்ள வன்பொருள்களுக்கு பங்கம் வராதபடி 'பாதுகாப்பாக' தேடு"

#: ../hwconf.c:902
msgid "set timeout in seconds"
msgstr "காலம் கடந்ததை வினாடிகளாக அமை"

#: ../hwconf.c:905
msgid "probe only, print information to stdout"
msgstr "stdout இல் அச்சுத்தகவல்களை மட்டும் அனுப்பும்படி தேடு"

#: ../hwconf.c:909
msgid "probe only the specified 'bus'"
msgstr "குறிப்பிட்ட பஸ்ஸை மட்டும் தேடு"

#: ../hwconf.c:913
msgid "probe only for the specified 'class'"
msgstr "வகுப்புத்தகவலை மட்டும் தேடு"

#: ../hwconf.c:917
msgid "read probed hardware from a file"
msgstr "கோப்பில் இருந்து வன்பொருள் தகவலை படி அல்லது தேடு"

#: ../hwconf.c:918
msgid "file to read hardware info from"
msgstr "வன்பொருள் தகவலை தேடுவதற்கான கோப்பு"

#: ../hwconf.c:921
msgid "search for modules for a particular kernel version"
msgstr "குறிப்பிட்ட கர்னலின் பதிப்பை மட்டும் தேடு"

#: ../hwconf.c:922
msgid "kernel version"
msgstr "கர்னல் பதிப்பு"

#: ../hwconf.c:925
msgid "read probed hardware from a socket"
msgstr "சாக்கெட்டிலிருந்து தேடிய வன்பொருளை படி"

#: ../hwconf.c:929
msgid "do configuration that doesn't require user input"
msgstr ""

#: ../hwconf.c:951
#, c-format
msgid ""
"\n"
"ERROR - You must be root to run kudzu.\n"
msgstr ""
"\n"
"ERROR - குடுவிற்கு அனுப்பு.\n"

